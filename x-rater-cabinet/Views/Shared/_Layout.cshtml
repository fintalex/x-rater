<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.slate.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.1.3.js"></script>
    <script src="~/Scripts/angular.js"></script>

    <script src="~/Scripts/modernizr-2.6.2.js"></script>

	@*подключение главных файлов для начальной структуры ангуляра*@
    <script src="~/Scripts/cabApp/xraterApp.js"></script>
	<script src="~/Scripts/cabApp/services/raterServices.js"></script>
</head>
<body ng-app="xraterApp">
   
	@*  =====================   Шапка =====================  *@

	<div class="navbar navbar-default navbar-fixed-top">
		<div class="container">

			<div class="navbar-header">
				@Html.ActionLink("X-RATER", "Default", "Home") @*<a href="#" class="navbar-brand">X-RATER</a>*@
			</div>

			<ul class="nav navbar-nav">
				<li class="dropdown">
					<a href="#" class="dropdown-toggle" data-toggle="dropdown" id="myList" aria-expanded="true">Pages<span class="caret"></span></a>
					<ul class="dropdown-menu" aria-labelledby="myList">
						<li><a href="#">Default</a></li>
						<li class="divider"></li>
						<li><a href="#">Home</a></li>
						<li><a href="#">Index</a></li>
						<li><a href="#">Snake</a></li>
					</ul>
				</li>
				<li><a href="#">Help</a></li>
				<li>@Html.ActionLink("График", "Show", "Grafic")</li>
				<li><a href="#">Download</a></li>
			</ul>
			<ul class="nav navbar-nav navbar-right">
				<li><a href="#">Git</a></li>
				<li><a href="#">Contact</a></li>
			</ul>
		</div>
	</div>
	@*  ====================================================  *@

    <div class="container body-content">
        @RenderBody()

        <hr />

    </div>
	<script type="text/javascript">
		var MAIN_ROOT = location.origin + "/Services/";
	</script>
</body>
</html>